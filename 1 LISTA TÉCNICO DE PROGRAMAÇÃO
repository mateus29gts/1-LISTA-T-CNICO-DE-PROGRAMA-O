Exercicio 5:
A=float(input("Digite o seu peso para o calculo de IMC:"))
B=float(input("Digite a sua altura para o calculo de IMC:"))
result = A/B**2
print("O seu indice de Massa Corporal:%.2f"% resultado)
Exercicio 6:
raio=float(input("Qual é o raio da esfera? "))
raiz=3.14
vol= (4/3)*raiz*(raio**3)
ar=4*raiz*(raio**2)
print("O volume da esfera é ", vol, "A area da esfera é ", ar)
Exercicio 7:^
A=float(input("Digite o valor da sua prova 1 para o calculo:"))
	B=float(input("Digite o valor da sua prova 2 para o calculo:"))
	C=float(input("Digite o valor da sua nota atribuida para o calculo:"))
	result = 4*A+3*B+2*C	
    DEFINITIVO = result/9
	print("Média:%.2f"% DEFINITIVO 
Exercicio 8:
T=float(input("Digite o valor da seu tempo:"))
	s0= 2
	v0 = 3
	A = 10
	result = s0+v0*T+0.5*A*T**2
	#print("seu valor é :%.2f"% result)
	print('O valor é de {}'.format(result))
Exercicio 9:
preco_maca=1.30#preço por unidade
preco_banana=5#preço por kilo
m=float(input("Qual é a quantidade de maças? "))
b=float(input("Qual a quantidade em kilo da bananas? "))
total_compras= (m*preco_maca)+(b*preco_banana)
print("o custo total das compras é ", total_compras)
Exercicio 10:
id_anos=float(input("digite respectivamente sua idade em anos,messes e dias: "))
id_meses=float(input(" "))
id_dias=float(input(" "))
total=(id_anos*365)+(id_meses*30)+id_dias
print("Sua idade em dias é ",total)
Exercicio 11:
custo_de_fabrica=float(input("Digite o valor de custo do carro de fabrica "))
distri=custo_de_fabrica*0.28
impost=custo_de_fabrica*0.45
custo_final=custo_de_fabrica+distri+impost
print("O custo total do carro é ",custo_final)
Exercicio 12:
carros_vendidos=float(input("Qual o numero de carros vendidos pela funcionaria? "))#
valor_vendas=float(input("Qual o valor total de vendas da  funcionaria? "))
salario_fixo=float(input("Qual o salario fixo da funcionaria? "))
comissão=float(input("Qual o valor da comissão recebida pela funcionaria? "))
salario_final=salario_fixo+(comissão*carros_vendidos)+((5*valor_vendas)/100)
print("O salario final da funcionaria é ",salario_final)
